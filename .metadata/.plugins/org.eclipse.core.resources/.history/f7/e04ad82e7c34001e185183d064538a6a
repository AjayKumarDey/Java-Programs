package String;

import java.util.Scanner;

public class Vowel {
public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	System.out.println("enter the char ");
	char ch[]=new char[5];
	for(int i=0;i<ch.length;i++)
	{
		ch[i]=sc.nextLine().charAt(0);
	}
	for(int i=0;i<ch.length;i++)
	{
		System.out.println(ch[i]+" ");
	}
	
}
}
