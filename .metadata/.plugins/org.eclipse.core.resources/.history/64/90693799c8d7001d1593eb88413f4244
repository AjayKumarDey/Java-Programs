      System.out.println("enter the number");
	Scanner sc=new Scanner(System.in);
	int n=sc.nextInt();
	/* for (int i = 1; i <=n; i=i+2) */
	for(int i=0;i<=n;i=i+1)
{
		
	System.out.println(i+" ");
}

}
}
